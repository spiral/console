{
    "name": "spiral/console",
    "type": "library",
    "description": "Console Core, Auto-Discovery, Command Sequencing",
    "license": "MIT",
    "homepage": "https://spiral.dev",
    "support": {
        "issues": "https://github.com/spiral/framework/issues",
        "source": "https://github.com/spiral/console"
    },
    "authors": [
        {
            "name": "Anton Titov (wolfy-j)",
            "email": "wolfy-j@spiralscout.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "spiral/core": "^2.15",
        "symfony/console": "^5.3|^6.0"
    },
    "require-dev": {
        "spiral/boot": "^2.15",
        "symfony/tokenizer": "^2.10",
        "phpunit/phpunit": "^8.5|^9.5",
        "mockery/mockery": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "Spiral\\Console\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spiral\\Tests\\Console\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.15.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
