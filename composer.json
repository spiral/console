{
    "name": "spiral/console",
    "type": "library",
    "description": "Console Core, Auto-Discovery, Command Sequencing",
    "license": "MIT",
    "homepage": "https://spiral.dev",
    "support": {
        "issues": "https://github.com/spiral/framework/issues",
        "source": "https://github.com/spiral/console"
    },
    "authors": [
        {
            "name": "Anton Titov (wolfy-j)",
            "email": "wolfy-j@spiralscout.com"
        },
        {
            "name": "Pavel Butchnev (butschster)",
            "email": "pavel.buchnev@spiralscout.com"
        },
        {
            "name": "Aleksei Gagarin (roxblnfk)",
            "email": "alexey.gagarin@spiralscout.com"
        },
        {
            "name": "Maksim Smakouz (msmakouz)",
            "email": "maksim.smakouz@spiralscout.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "spiral/core": "^3.12",
        "spiral/events": "^3.12",
        "spiral/hmvc": "^3.12",
        "spiral/tokenizer": "^3.12",
        "symfony/console": "^6.1 || ^7.0"
    },
    "require-dev": {
        "spiral/boot": "^3.12",
        "phpunit/phpunit": "^10.1",
        "mockery/mockery": "^1.5",
        "vimeo/psalm": "^5.9"
    },
    "autoload": {
        "psr-4": {
            "Spiral\\Console\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spiral\\Tests\\Console\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.12.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
